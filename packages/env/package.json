{
    "name": "@mindplanr/env",
    "version": "0.1.0",
    "private": true,
    "description": "Mindplanr's typesafe ENV package",
    "author": "Joshua Epstein",
    "type": "module",
    "exports": {
        ".": "./dist/index.js"
    },
    "main": "./dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "dev": "pnpm run build --watch",
        "lint": "eslint . --max-warnings 0",
        "lint:fix": "pnpm run lint --fix",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@t3-oss/env-nextjs": "^0.6.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@mindplanr/tsconfig": "workspace:^",
        "@mindplanr/eslint": "workspace:^"
    },
    "volta": {
        "extends": "../../package.json"
    }
}